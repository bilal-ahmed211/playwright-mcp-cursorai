{
  "/* $schema */": "Schema validation disabled due to connection issues",
  "name": "fyiai_playwright-ui",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc --build --clean",
    "build:watch": "tsc --watch",
    "install:browsers": "npx playwright install",
    "test": "npx playwright test",
    "test:web": "npx playwright test src/tests/web/",
    "test:api": "npx playwright test src/tests/api/",
    "test:petstore": "npx playwright test src/tests/api/PetStore/",
    "test:fyiai": "npx playwright test src/tests/api/FyiAi/ src/tests/web/FyiAi/",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
    "test:ui": "npx playwright test --ui",
    "test:ci": "npx playwright test --reporter=line,allure-playwright",
    "test:parallel": "npx playwright test --workers=4",
    "allure:generate": "allure generate ./allure-results -o ./allure-report --clean",
    "allure:open": "allure open ./allure-report",
    "allure:serve": "allure serve ./allure-results",
    "generate:api:report": "npm run build && npx ts-node generate-api-reports.ts",
    "enhance:report": "npm run build && node generate-reports.js",
    "open:exec-summary:report": "start dashboards/latest-summary.html",
    "open:dashboard": "start dashboards/trends-dashboard.html",
    "open:api:performance:report": "npm run build && start dashboards/api/api-performance-dashboard.html",
    "open:api:compliance:report": "npm run build && start dashboards/api/api-contract-compliance.html",
    "open:api:coverage:report": "npm run build && start dashboards/api/api-coverage-analysis.html"

  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
