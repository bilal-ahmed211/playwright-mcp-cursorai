    "@testcontainers/postgresql": "^10.24.2",
    "@types/mongodb": "^4.0.6",
    "@types/mssql": "^9.1.7",
    "@types/node": "^22.15.2",
    "@types/oracledb": "^6.6.0",
    "@types/pg": "^8.11.14",
    "allure-playwright": "^3.2.1",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "mongodb": "^6.16.0",
    "mssql": "^11.0.1",
