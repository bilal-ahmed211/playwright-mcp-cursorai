{
  "name": "fyiai_playwright-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:web": "npx playwright test src/tests/web/",
    "test:api": "npx playwright test src/tests/api/",
    "test:petstore": "npx playwright test src/tests/api/PetStore/",
    "test:fyiai": "npx playwright test src/tests/api/FyiAi/ src/tests/web/FyiAi/",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
    "test:ui": "npx playwright test --ui",
    "test:ci": "npx playwright test --reporter=line,allure-playwright",
    "test:parallel": "npx playwright test --workers=4",
    "allure:generate": "allure generate ./allure-results -o ./allure-report --clean",
    "allure:open": "allure open ./allure-report",
    "allure:serve": "allure serve ./allure-results",
    "install:browsers": "playwright install",
    "report": "npm run allure:generate && npm run allure:open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@faker-js/faker": "^9.7.0",
    "@playwright/test": "^1.52.0",
    "@types/node": "^22.15.2",
    "allure-playwright": "^3.2.1",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "xlsx": "^0.18.5"
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/fs-extra": "^11.0.4",
    "axios": "^1.9.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "fs-extra": "^11.3.0"
  }
}
